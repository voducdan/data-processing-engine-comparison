# Example configuration for benchmark script

# Hive Metastore connection
export HIVE_METASTORE_URI="thrift://localhost:9083"

# Table to benchmark
export TABLE_NAME="warehouse.default.sample_table"

# Query to execute (or pass as first command line argument)
export QUERY="SELECT COUNT(*) as total_rows, AVG(price) as avg_price FROM warehouse.default.sample_table"

# StarRocks connection settings (if using StarRocks)
export STARROCKS_HOST="localhost"
export STARROCKS_PORT="9030"
export STARROCKS_USER="root"
export STARROCKS_PASSWORD=""
export STARROCKS_DATABASE="iceberg_catalog"
